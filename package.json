{
  "name": "office-tool-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/nachetfalter/office-tool-backend.git",
  "author": "Victor Wang",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project ./",
    "start": "nodemon ./src/app.ts",
    "start:serverless": "serverless offline",
    "lint": "eslint --fix --ext .ts .",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 85,
        "functions": 85,
        "lines": 85
      }
    }
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/fs-extra": "^9.0.13",
    "@types/jimp": "^0.2.28",
    "@types/jszip": "^3.4.1",
    "@types/multer": "^1.4.7",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "fs-extra": "^10.0.0",
    "gm": "^1.23.1",
    "jimp": "^0.16.1",
    "jszip": "^3.7.1",
    "multer": "^1.4.4",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^2.1.4",
    "rimraf": "^3.0.2",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.1-alpha.17",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-typescript": "^2.1.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
